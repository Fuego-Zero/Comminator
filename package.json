{
  "name": "comminator",
  "version": "1.0.0",
  "description": "Summarize git commits into daily reports using AI. Terminate wasted time.",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "comminator": "./src/index.ts"
  },
  "scripts": {
    "start": "npm run report",
    "report": "node --no-warnings --import tsx ./src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "git",
    "ai",
    "daily-report",
    "ollama",
    "cli",
    "automation",
    "productivity"
  ],
  "author": "Fuego",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tslib": "^2.8.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
